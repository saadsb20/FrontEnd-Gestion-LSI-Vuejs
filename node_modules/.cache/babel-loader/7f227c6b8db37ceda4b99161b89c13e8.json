   }\r\n},\r\n{\r\n    path: '/etudiant',\r\n    name: 'etudiant',\r\n    component: EtudiantVue,\r\n    beforeEnter: (to, from, next) => {\r\n        if (store.state.user_role != \"Student\") {\r\n            next('/forbidden');\r\n        } else {\r\n            next();\r\n        }\r\n    }\r\n},\r\n{\r\n    path: '/Login',\r\n    name: 'Login',\r\n    component: Login\r\n},\r\n{\r\n    path: '/forbidden',\r\n    name: 'access',\r\n    component: Access\r\n},\r\n{\r\n    path: '/e_registre',\r\n    name: 'Etudiant',\r\n    component: EtudiantRegistre,\r\n    beforeEnter: (to, from, next) => {\r\n        if (store.state.user_role != \"Admin\") {\r\n            next('/forbidden');\r\n        } else {\r\n            next();\r\n        }\r\n    }\r\n},\r\n{\r\n    path: '/p_registre',\r\n    name: 'Prof',\r\n    component: ProfRegistre\r\n},\r\n{\r\n    path: '/',\r\n    name: 'Home',\r\n    component: Home\r\n},\r\n{\r\n    path: '/CrudM',\r\n    name: 'CrudM',\r\n    component: CrudM\r\n},\r\n{\r\n    path: '/CrudEmploi',\r\n    name: 'CrudEmploi',\r\n    component: CrudEmploi\r\n}\r\n\r\n];\r\n\r\nconst router = new VueRouter({\r\n    routes: routes,\r\n    mode: 'history',\r\n});\r\n\r\nVue.config.productionTip = false\r\n\r\nnew Vue({\r\n    router,\r\n    store,\r\n    render: h => h(App),\r\n}).$mount('#app')"]}]}